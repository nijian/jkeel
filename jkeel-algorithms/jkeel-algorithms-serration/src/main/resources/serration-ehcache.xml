<config xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xmlns='http://www.ehcache.org/v3'
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">
    <cache alias="closure-cache">
        <key-type>java.lang.String</key-type>
        <value-type>groovy.lang.Closure</value-type>
        <resources>
            <heap unit="entries">10000</heap>
        </resources>

    </cache>
    <cache alias="algorithmTemplate-cache">
        <key-type>java.lang.String</key-type>
        <value-type>com.github.nijian.jkeel.algorithms.serration.entity.LayoutTemplate</value-type>
        <resources>
            <heap unit="entries">10000</heap>
        </resources>

    </cache>
</config>